{
	"name": "lisk-sample-app",
	"version": "0.0.0",
	"description": "Sample app using latest SDK",
	"author": "Lisk Foundation <admin@lisk.io>, lightcurve GmbH <admin@lightcurve.io>",
	"license": "Apache-2.0",
	"keywords": [
		"cryptocurrency",
		"blockchain",
		"lisk",
		"nodejs",
		"javascript"
	],
	"homepage": "https://github.com/LiskHQ/lisk-sdk#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/LiskHQ/lisk-sdk.git"
	},
	"bugs": {
		"url": "https://github.com/LiskHQ/lisk-sdk/issues"
	},
	"engines": {
		"node": ">=12.13.0 <=12",
		"npm": ">=6.12.0"
	},
	"files": [
		"/dist",
		"/config",
		"/npm-shrinkwrap.json",
		"/oclif.manifest.json",
		"/yarn.lock"
	],
	"oclif": {
		"commands": "./dist/commands",
		"bin": "lisk-sample-app",
		"dirname": "~/.lisk-sample-app",
		"devPlugins": [
			"@oclif/plugin-help"
		]
	},
	"scripts": {
		"build": "tsc",
		"postpack": "rm -f oclif.manifest.json",
		"posttest": "eslint . --ext .ts --config .eslintrc",
		"prepack": "rm -rf dist && tsc -b && oclif-dev manifest && oclif-dev readme",
		"test": "echo NO TESTS",
		"version": "oclif-dev readme && git add README.md"
	},
	"dependencies": {
		"@liskhq/lisk-cryptography": "2.4.2",
		"@liskhq/lisk-transactions": "3.0.2",
		"@liskhq/lisk-validator": "0.3.1",
		"@oclif/command": "1.5.19",
		"@oclif/config": "1.14.0",
		"fs-extra": "8.1.0",
		"lisk-framework": "0.5.2",
		"lisk-sdk": "3.0.2",
		"tslib": "1.11.1"
	},
	"devDependencies": {
		"@oclif/dev-cli": "1.22.2",
		"@oclif/plugin-help": "2.2.3",
		"@types/node": "12.12.11",
		"globby": "11.0.0",
		"ts-node": "8.6.2",
		"tslint": "6.1.0",
		"tslint-config-prettier": "1.18.0",
		"tslint-immutable": "6.0.1",
		"typescript": "3.8.3"
	}
}
